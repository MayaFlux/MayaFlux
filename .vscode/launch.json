{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug MayaFlux (Linux/macOS)",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/MayaFlux",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set disassembly flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "Build MayaFlux (Debug)",
      "osx": {
        "MIMode": "lldb",
        "program": "${workspaceFolder}/build/MayaFlux",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for lldb",
            "text": "settings set target.inline-breakpoint-strategy always",
            "ignoreFailures": true
          }
        ]
      }
    },
